#pragma config(Motor,  port2,           leftMotor,     tmotorVex393_MC29, openLoop)
#pragma config(Motor,  port6,           rightMotor,    tmotorVex393_HBridge, openLoop, reversed, encoderPort, None)
//*!!Code automatically generated by 'ROBOTC' configuration wizard               !!*//

/*------------------------------------------------------------------------------------------------
  This program will move your robot forward for 5 seconds. Stop for 2 seconds, spin, then back up for 5 seconds.  At the end of the program, all motors
  will shut down automatically and turn off.

                            ROBOT CONFIGURATION: VEX IQ Clawbot
    MOTORS & SENSORS:
    [I/O Port]          [Name]              [Type]                			[Location]
    Port 2        			leftMotor           VEX IQ Motor		      Left side motor
    Port 6       				rightMotor          VEX IQ Motor		      Right side motor (reversed)
------------------------------------------------------------------------------------------------*/

task main()
{
	setMotorSpeed(leftMotor, 50);		//Set the leftMotor (motor1) to forward half power (50)
	setMotorSpeed(rightMotor, 50);  //Set the rightMotor (motor6) to forward half power (50)
	sleep(5000);  //Wait for 5 seconds before continuing on in the program.

	setMotorSpeed(leftMotor, 0);		//Set the leftMotor (motor1) to zero power (0)
	setMotorSpeed(rightMotor, 0);  //Set the rightMotor (motor6) to zero power (0)
	sleep(2000);  //Wait for 2 seconds before continuing on in the program.

	setMotorSpeed(leftMotor, 50);		//Set the leftMotor (motor1) to zero power (50)
	setMotorSpeed(rightMotor, -50);  //Set the rightMotor (motor6) to zero power (-50)
	sleep (9000);  //Wait for 9 seconds before continuing on in the program.

	setMotorSpeed(leftMotor, -50);		//Set the leftMotor (motor1) to half power (-50)
	setMotorSpeed(rightMotor, -50);  //Set the rightMotor (motor6) to half power (-50)
	sleep(5000);										//Wait for 2 seconds before continuing on in the program.
}
